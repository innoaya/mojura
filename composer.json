    "keywords": [
        "innoaya",
        "laravel",
        "mojura",
        "modular"
    ],
    "homepage": "https://github.com/innoaya",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Aung Kyaw Minn",
            "email": "ajminn207@gmail.com"
        }
    ],
    "require": {
